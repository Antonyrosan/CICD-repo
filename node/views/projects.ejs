<!DOCTYPE html>
<html>
<head>
  <title>Projects - <%= data.name %></title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <header>
    <h1>Projects</h1>
    <p>DevOps Projects | Real World Infrastructure Automation</p>
  </header>

  <nav>
    <a href="/">Home</a>
    <a href="/about">About</a>
    <a href="/projects">Projects</a>
    <a href="/contact">Contact</a>
  </nav>

  <main>
    <% data.projects.forEach(project => { %>
      <div class="project">
        <h2><%= project.title %></h2>
        <p><%= project.description %></p>
      </div>
    <% }) %>
  </main>

  <footer>
    &copy; <%= new Date().getFullYear() %> <%= data.name %>
  </footer>
</body>
</html>
